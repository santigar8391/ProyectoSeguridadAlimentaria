CREATE DATABASE proyectoSeguridad;

USE proyectoSeguridad;

--CREACIÃ“N DE TABLAS

CREATE TABLE IF NOT EXISTS producto (
    idproducto int(11) NOT NULL AUTO_INCREMENT,
    descproducto varchar(30) DEFAULT NULL,
    PRIMARY KEY(idproducto)
    );
    --engine=TokuDB;

